{
    "routes": [
        {
            "route": "/dashboard",
            "allowedRoles": ["wooduser"]
        },
        {
            "route": "/requestform",
            "allowedRoles": ["workshopuser","wooduser"]
        },
        {
            "route": "/reportform",
            "allowedRoles": ["wooduser"]
        },
        {
            "route": "/scripts/dashboardcode.js",
            "methods": ["GET"],
            "allowedRoles": ["wooduser","workshopuser"]
        },
        {
            "route": "/scripts/requestformcode.js",
            "methods": ["GET"],
            "allowedRoles": ["wooduser","workshopuser"]
        },
        {
            "route": "/scripts/reportformcode.js",
            "methods": ["GET"],
            "allowedRoles": ["wooduser"]
        },
        {
            "route": "/images",
            "methods": ["GET"],
            "allowedRoles": ["wooduser","workshopuser"]
        },
        {
            "route": "/styles/*",
            "methods": ["GET"],
            "allowedRoles": ["wooduser","workshopuser"]
        },
        {
            "route": "/login",
            "rewrite": "/.auth/login/aad"
        },
        {
            "route": "/.auth/login/twitter",
            "statusCode": 404
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        }
    ],
    "responseOverrides": {
      "400": {
        "rewrite": "/invalid-invitation-error.html"
      },
      "401": {
        "redirect": "/login",
        "statusCode": 302
      },
      "403": {
        "rewrite": "/custom-forbidden-page.html"
      },
      "404": {
        "rewrite": "/404.html"
      }
    },
    "globalHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }